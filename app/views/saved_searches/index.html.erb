<div class="row">
  <div class="col-sm-6">
      <div id="mapSavedSearch"></div>
        <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDwX5lbHn1JS_1JHy1DB5Nmt9GMUPSMB18" async defer>
        </script>
  </div>

  <div id="app">
  <!-- Charts JS -->
    <div class="col-sm-6">
        <!-- <canvas id="mix2" count="2"></canvas> -->
        <!-- <chartjs-bar></chartjs-bar> -->
        <chartjs-line :labels="['Burley', 'Hawthorne', 'Lake View High School']"></chartjs-line>
    </div>
  </div>
</div>
<br>
<!-- List of Saved schools -->
  <div class="well">
    <h3><%= current_user.name %>'s saved searches</h3><br>
    <div class="row">
    <% @saved_schools.each do |school| %>
      <div class="col-md-4">
        <div class="well">
          <h4><a href="/schools/<%= school['school_id'] %>"><%= school["long_name"] %></a></h4><br>
          <p>-<%= school["student_growth_rating"] %></p>
          <p>-<%= school["student_attainment_rating"] %></p>
          <p>-<%= school["cultural_climate_rating"] %></p>
          <p>-<%= school["healthy_school_certification"] %></p>
          <p>-<%= school["creative_school_certification"] %></p>
          <p>-<%= school["student_attendance_avg_pct"] %></p>
          <p>-<%= school["teacher_attendance_avg_pct"] %></p>
          <p>-<%= school["freshmen_on_track"] %></p>
          <p>-<%= school["supportive_school_award"] %></p>
          <p>-<%= school["long_name"] %></p>
          <p>-<%= school["long_name"] %></p>
          <p>-<%= school["long_name"] %></p>
          <p>-<%= school["long_name"] %></p>
          <p>-<%= school["long_name"] %></p>
          <p>-<%= school["long_name"] %></p>
          <p>-<%= school["long_name"] %></p>
          <p>-<%= school["long_name"] %></p><br>
          <% if current_user %> 
            <%= form_tag "/savedsearch/#{school['school_id']}", method: "delete" do %>
              <input type="submit" class="btn btn-default" value="Remove school">
            <% end %>
          <% end %>

        </div>
      </div>
    <% end %>
    </div>
  </div>