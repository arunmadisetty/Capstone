<div id="map"></div>
<script>
var map;
function initMap() {
  map = new google.maps.Map(document.getElementById('map'), {
    center: {lat: -34.397, lng: 150.644},
    zoom: 8
  });
}
 </script>
 <script src="https://maps.googleapis.com/maps/api/js?callback=initMap&key=AIzaSyDwX5lbHn1JS_1JHy1DB5Nmt9GMUPSMB18"
 async defer></script>


<div id="app">
  {{ message }}

  <h2><%= current_user.name %>'s saved searches</h2>
  <transition-group name="bounceIn">
    <div v-for="ringer in ringers" v-bind:key="ringer.id">
      <h3>{{ ringer.character }} | {{ ringer.location }}</h3>
    </div>
  </transition-group>

  <% @saved_schools.each do |school| %>
    <p><%= school["long_name"] %></p>
  <% end %>
</div>