<div class="container">
  <h2><%= @school_primary.first["long_name"] %></h2>
  <h5><%= @school_primary.first["summary"] %></h5>
  <h3>About the neighborhood</h3>
  <div>
    <%= form_tag "/savedsearch", method: "post" do %>
      <input type="hidden" class="form-control" name="form_product_id" value="<%= @school_primary.first["school_id"] %>">
      <input type="submit" class="btn btn-default" value="Select to compare">
    <% end %>
  </div>

  
</div>